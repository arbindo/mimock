{
    "name": "mimock-ui",
    "version": "0.0.1",
    "scripts": {
        "format:check": "prettier --check --config=.prettierrc src/**/*.js src/**/*.jsx",
        "format:file": "prettier --write --config=.prettierrc ",
        "format:fix": "prettier --write --config=.prettierrc .",
        "lint:file": "eslint ",
        "lint:all": "eslint --config=.eslintrc.js src/**/*.js src/**/*.jsx",
        "build": "webpack --mode production",
        "build:dev": "webpack --mode development",
        "build:start": "cd dist && PORT=3000 npx serve",
        "start": "webpack serve --open --mode development",
        "start:live": "webpack serve --open /mimock-ui/ --mode development --live-reload --hot",
        "test": "jest --silent=true",
        "pre-push:check": "yarn format:check && yarn lint:all && yarn test"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "mimock-dev",
        "email": "contact@mimock.io"
    },
    "devDependencies": {
        "@babel/core": "^7.17.4",
        "@babel/plugin-transform-runtime": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-react": "^7.14.5",
        "autoprefixer": "^10.1.0",
        "axios-mock-adapter": "^1.20.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-jsx-control-statements": "^4.1.1",
        "babel-plugin-module-resolver": "^4.1.0",
        "css-loader": "^6.3.0",
        "eslint": "^8.9.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jest": "^26.1.1",
        "eslint-plugin-jsx-control-statements": "^2.2.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.26.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.3.2",
        "husky": "^8.0.1",
        "jest-mock-axios": "^4.5.0",
        "lint-staged": "^12.4.0",
        "postcss": "^8.2.13",
        "postcss-loader": "^6.2.1",
        "style-loader": "^3.3.0",
        "tailwind-styled-components": "^2.1.4",
        "tailwindcss": "^3.0.22",
        "typescript": "^4.6.3",
        "webpack": "^5.69.0",
        "webpack-cli": "^4.9.0",
        "webpack-dev-server": "^4.3.1"
    },
    "dependencies": {
        "@babel/runtime": "^7.13.10",
