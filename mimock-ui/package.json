{
    "name": "mimock-ui",
    "version": "0.0.1",
    "scripts": {
        "format:check": "prettier --check --config=.prettierrc src/**/*.js src/**/*.jsx",
        "format:fix": "prettier --write --config=.prettierrc .",
        "lint:file": "eslint ",
        "lint:all": "eslint --config=.eslintrc.js src/**/*.js src/**/*.jsx",
        "build": "webpack --mode production",
        "build:dev": "webpack --mode development",
        "build:start": "cd dist && PORT=3000 npx serve",
        "start": "webpack serve --open --mode development",
        "start:live": "webpack serve --open /mimock-ui/ --mode development --live-reload --hot",
        "test": "jest --silent=true",
        "pre-commit:check": "yarn format:check && yarn lint:all"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "mimock-dev",
        "email": "contact@mimock.io"
    },
    "devDependencies": {
